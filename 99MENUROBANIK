AddButton("Bring", "old radio", function()
local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local StarterGui = game:GetService("StarterGui")

local LocalPlayer = Players.LocalPlayer
local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
local HRP = Character:WaitForChild("HumanoidRootPart")

local object = workspace.Items["Old Radio"]:WaitForChild("Main")

local aboveOffset = Vector3.new(0, 50, 0)
local objectBelowOffset = Vector3.new(0, -20, 0)
local tweenTime = 0.1

local function teleportPart(part, position, time)
    if part and part.Parent then
        local tween = TweenService:Create(part, TweenInfo.new(time, Enum.EasingStyle.Linear), {CFrame = CFrame.new(position)})
        tween:Play()
        tween.Completed:Wait()
    end
end

local function countdown(seconds)
    for i = seconds, 1, -1 do
        StarterGui:SetCore("SendNotification", {
            Title = "–í—Ä–µ–º—è –Ω–∞ —Å–±–æ—Ä –æ–±—ä–µ–∫—Ç–∞",
            Text = tostring(i),
            Duration = 1
        })
        wait(1)
    end
end

local function teleportAndCountdown()
    local originalPos = HRP.Position
    teleportPart(HRP, object.Position, tweenTime)
    teleportPart(HRP, object.Position + aboveOffset, tweenTime)
    teleportPart(object, HRP.Position + objectBelowOffset, tweenTime)
    countdown(5)
    teleportPart(HRP, originalPos, tweenTime)
end

teleportAndCountdown()
end)

AddButton("Bring", "broken fen", function()
local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local StarterGui = game:GetService("StarterGui")

local LocalPlayer = Players.LocalPlayer
local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
local HRP = Character:WaitForChild("HumanoidRootPart")

local object = workspace.Items["Broken Fan"]:WaitForChild("Main")

local aboveOffset = Vector3.new(0, 50, 0)
local objectBelowOffset = Vector3.new(0, -20, 0)
local tweenTime = 0.1

local function teleportPart(part, position, time)
    if part and part.Parent then
        local tween = TweenService:Create(part, TweenInfo.new(time, Enum.EasingStyle.Linear), {CFrame = CFrame.new(position)})
        tween:Play()
        tween.Completed:Wait()
    end
end

local function countdown(seconds)
    for i = seconds, 1, -1 do
        StarterGui:SetCore("SendNotification", {
            Title = "–í—Ä–µ–º—è –Ω–∞ —Å–±–æ—Ä –æ–±—ä–µ–∫—Ç–∞",
            Text = tostring(i),
            Duration = 1
        })
        wait(1)
    end
end

local function teleportAndCountdown()
    local originalPos = HRP.Position
    teleportPart(HRP, object.Position, tweenTime)
    teleportPart(HRP, object.Position + aboveOffset, tweenTime)
    teleportPart(object, HRP.Position + objectBelowOffset, tweenTime)
    countdown(5)
    teleportPart(HRP, originalPos, tweenTime)
end

teleportAndCountdown()
end)

AddButton("Bring", "old microwave", function()
local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local StarterGui = game:GetService("StarterGui")

local LocalPlayer = Players.LocalPlayer
local Character = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
local HRP = Character:WaitForChild("HumanoidRootPart")

-- –û–±—ä–µ–∫—Ç: Broken Microwave
local object = workspace.Items["Broken Microwave"]:WaitForChild("Main")

local aboveOffset = Vector3.new(0, 50, 0)       -- –∏–≥—Ä–æ–∫ –≤—ã—à–µ –æ–±—ä–µ–∫—Ç–∞
local objectBelowOffset = Vector3.new(0, -20, 0) -- –æ–±—ä–µ–∫—Ç –Ω–∏–∂–µ –∏–≥—Ä–æ–∫–∞
local tweenTime = 0.1

-- –ü–ª–∞–≤–Ω—ã–π —Ç–µ–ª–µ–ø–æ—Ä—Ç
local function teleportPart(part, position, time)
    if part and part.Parent then
        local tween = TweenService:Create(
            part,
            TweenInfo.new(time, Enum.EasingStyle.Linear),
            {CFrame = CFrame.new(position)}
        )
        tween:Play()
        tween.Completed:Wait()
    end
end

-- –û–±—Ä–∞—Ç–Ω—ã–π –æ—Ç—Å—á—ë—Ç
local function countdown(seconds)
    for i = seconds, 1, -1 do
        StarterGui:SetCore("SendNotification", {
            Title = "–í—Ä–µ–º—è –Ω–∞ —Å–±–æ—Ä –æ–±—ä–µ–∫—Ç–∞",
            Text = tostring(i),
            Duration = 1
        })
        wait(1)
    end
end

-- –û—Å–Ω–æ–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è
local function teleportAndCountdown()
    local originalPos = HRP.Position

    -- 1. –¢–µ–ª–µ–ø–æ—Ä—Ç –∫ –æ–±—ä–µ–∫—Ç—É
    teleportPart(HRP, object.Position, tweenTime)

    -- 2. –ü–æ–¥–Ω—è—Ç–∏–µ –∏–≥—Ä–æ–∫–∞, –æ–±—ä–µ–∫—Ç –Ω–∏–∂–µ
    teleportPart(HRP, object.Position + aboveOffset, tweenTime)
    teleportPart(object, HRP.Position + objectBelowOffset, tweenTime)

    -- 3. –û—Ç—Å—á—ë—Ç 5..1
    countdown(5)

    -- 4. –í–æ–∑–≤—Ä–∞—Ç –∏–≥—Ä–æ–∫–∞
    teleportPart(HRP, originalPos, tweenTime)
end

-- –ó–∞–ø—É—Å–∫
teleportAndCountdown()
end)

